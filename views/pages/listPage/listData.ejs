<% listData.map(function(value){ %>
<div class="listjob">
  <a
    class="list-detail"
    style="background: <%= value.color %>"
    href="/list/<%= value._id %>"
    ><%= value.listName %></a
  >
  <div class="btn-click">
    <button class="btn-view update">Update</button>
    <button class="btn-view" onclick="deleteList('<%= value._id %>')">
      Remove
    </button>
  </div>
</div>
<% }) %>
